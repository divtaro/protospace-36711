<div class="card">
  <%= link_to image_tag(prototype.image, class: :card__img ), prototype_path(prototype.id), method: :get %>
  <div class="card__body">
    <%= link_to prototype.title, prototype_path(prototype.id), class: :card__title%>
    <p class="card__summary">
      <%= link_to prototype.catch_copy %>
    </p>
     <%# user_pathをuser_path(current_user)に変更した %>

    <%= link_to "by #{prototype.user.name}", user_path(prototype.user_id), class: :card__user %>
  </div>
</div>